<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:id="@+id/ll_alarmAnalysis"
    tools:context=".ui.pressureAnalysis.PressureAnalysisActivity">
    <com.ogaclejapan.smarttablayout.SmartTabLayout
        android:id="@+id/tab_alarmAnalysis"
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:paddingBottom="4dp"
        app:stl_defaultTabTextColor="@color/black"
        app:stl_distributeEvenly="true"
        app:stl_indicatorColor="@color/blueberryColor"
        app:stl_indicatorCornerRadius="1.5dp"
        app:stl_indicatorInterpolation="smart"
        app:stl_indicatorThickness="3dp"
        app:stl_underlineColor="@color/transparent"
        />

    <android.support.v4.view.ViewPager
        android:id="@+id/vp_alarmAnalysis"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >
    </android.support.v4.view.ViewPager>


</LinearLayout>